@page
@model SchoolMgtWebApp.Pages.Admin.Students.IndexModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}


<div class="border container backgroundWhite">
    <div class="row">
        <div class="col-md-6">
            <h2 class="text-primary">Registered Students</h2>
        </div>
        <div class="col-md-6 text-right">
            
        </div>
    </div>
    <br />
    <table id="dataLoad" class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Matric No</th>
                <th>Dept</th>
                <th>Level</th>
                <th>Action</th>
            </tr>
        </thead>
    </table>
</div>

@section scripts{
    <script src="~/js/students.js"></script>
}




